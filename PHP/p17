<!-- Write a PHP function to check a number is prime or not -->

<?php

function primeornot($a)
{
    $i = 1;
    $count = 0;
    while ($i<=$a) {
        if ($a%$i==0) {
            $count = $count + 1;

        }
        $i = $i + 1;
    }
    if ($count == 2) {
        echo "$a is prime number";
    }
    else {
        echo "$a is not prime number";
    }

}
echo primeornot(13)


?>
